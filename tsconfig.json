{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "baseUrl": "./src",
        "paths": {
            "@/*": ["*"],
            "@/components/*": ["components/*"]
        }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
